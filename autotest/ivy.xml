<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
            xmlns:m="http://ant.apache.org/ivy/maven.xsd">

    <info organisation="pro.miniapps" module="autotest"/>

    <configurations>
        <conf name="selenium" description="For selenium tests build"/>
    </configurations>

    <dependencies>
        <!-- Selenium -->
        <dependency org="org.seleniumhq.selenium" name="selenium-java" rev="3.12.0"/>
        <dependency org="junit" name="junit" rev="4.12"/>
        <dependency org="org.javassist" name="javassist" rev="3.18.1-GA"/>
        <dependency org="org.mortbay.jetty" name="jetty-util" rev="6.1.25"/>
        <dependency org="org.seleniumhq.selenium" name="selenium-api" rev="3.8.1"/>
        <dependency org="com.google.guava" name="guava" rev="23.0"/>
        <dependency org="org.apache.commons" name="commons-exec" rev="1.3"/>
    </dependencies>
</ivy-module>


